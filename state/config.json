{
  "project_name": "Example Project",
  "version": "1.0.0",
  "description": "Template configuration for state-machine skills",
  
  "endpoints": {
    "api": "https://api.example.com",
    "frontend": "https://app.example.com",
    "database": "postgresql://localhost:5432/mydb"
  },
  
  "credentials": {
    "api_key": "{{API_KEY}}",
    "test_user": {
      "email": "test@example.com",
      "password": "{{TEST_PASSWORD}}"
    }
  },
  
  "settings": {
    "require_approval_between_phases": true,
    "auto_log_sessions": true,
    "max_retries_per_task": 3
  },
  
  "rate_limits": {
    "api_calls_per_minute": 60,
    "concurrent_requests": 5
  },
  
  "execution_methods": {
    "http": {
      "template": "curl -X {{METHOD}} {{URL}} -H 'Authorization: Bearer {{API_KEY}}'"
    },
    "database": {
      "template": "psql {{DATABASE_URL}} -c '{{QUERY}}'"
    }
  }
}
